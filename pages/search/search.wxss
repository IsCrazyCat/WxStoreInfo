/* pages/index/search/search.wxss */
.search-title{
    margin:0 20rpx;
    height: 100%;
}
.search-bar{
    display: flex;
    position: relative;
    margin-bottom:14rpx;
}
.search{
    position: absolute;
    z-index: 2;
    left:16rpx;
    top:10rpx;
}
.cancel-icon{
    position: absolute;
    right:150rpx;
    z-index:2;
    top:10rpx;
}
.search-bar>input{
    width:508rpx;
    height:60rxp;
    border-radius:25rpx;
    background-color:#e9e8ee;
    padding-left:60rpx;
    font-size: 28rpx;
    vertical-align: middle;

}
.placeholder{
    font-size:28rpx;
    color: #d6d6d9;
}
.cancel{
    width:120rpx;
    height:54rpx;
    font-size:26rpx;
    line-height:54rpx;
}
.search-btn{
  width:120rpx;
  height:54rpx;
  font-size:26rpx;
  line-height:54rpx;
  color: #fff;
  background-color: #f23030;
}
.content{
    border-top:1rpx solid #e2e2e2;
}
.wrap-content{
    margin:33rpx 20rpx 0 20rpx;
}
.bod-style{
  border-bottom: 1rpx solid #e9e8ee;
  text-indent: 10rpx;
  padding-bottom: 15rpx;
}
.storeInfoView{
  flex: auto;   
  position: relative;  
  margin-top: 10%; 
  height:90%;
}
.storeInfoView scroll-view{  
  position: absolute;  
  height: 100%;
  bottom: 0;  
  left: 0;  
  right: 0;  
  top: 0;  
}  
.nav_section_items{
    display:flex;
    flex-direction: row;
    justify-content: space-between;
    padding:30rpx;
    border-bottom:1rpx solid #ddd;
    position: relative;
}

.nav_section_items .section_image{
    width:170rpx;
    height:158rpx;
}
.nav_section_items .section_image image{
    width:100%;
    height:100%;
}
.nav_section_items .section_cont{
    width:350rpx;
    font-size:26rpx;
    color:#a9a9a9;
    
}
.nav_section_items .section_cont view{
    overflow:hidden;
    text-overflow: ellipsis;
    white-space:nowrap;
    display: block;
}
.nav_section_items .section_cont .section_cont_intro{
    white-space:normal;
    display: -webkit-box;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
}
.nav_section_items .section_cont .section_cont_sub{
    font-size: 30rpx;
    line-height:60rpx;
    color:#000;
    margin-bottom:10rpx;
}
.nav_section_items .section_cont .section_cont_price{
    font-size:32rpx;
    color:#d24a58;
}
.section_address{
  padding-top: 10rpx;
}
.nav_section_items .section_book{
    width:80rpx;
    height:80rpx;
    font-size:26rpx;
    border-radius:100%;
    background:#d24a58;
    line-height: 80rpx;
    color:#fff;
    text-align: center;
    margin-top:35rpx;
}
.weui-loading {
  margin: 0 5px;
  width: 20px;
  height: 20px;
  display: inline-block;
  vertical-align: middle;
  -webkit-animation: weuiLoading 1s steps(12, end) infinite;
          animation: weuiLoading 1s steps(12, end) infinite;
  background: transparent url(data:image/svg+xml;base64,) no-repeat;
  background-size: 100%;
}

.weui-loadmore {
  width: 65%;
  margin: 1.5em auto;
  line-height: 1.6em;
  font-size: 14px;
  text-align: center;
}
.weui-loadmore__tips {
  display: inline-block;
  vertical-align: middle;
}
.already_loaded{
  text-align: center;
  width: 100%;
  height: 80rpx;
  line-height: 80rpx;
  font-size: 24rpx;
  color: #c6c6c6;
}







